<div class="feed">
  
   <% @cooperatives.each do |cooperative| %>
  
  <div class="item shadow">
   <%= link_to cooperative do %>

  <div class="image">
    <%= image_tag cooperative.image.url(:thumb) %>
  </div>
  <% end %>
    <p><strong>name :</strong> <%= cooperative.name %></p>
        <div class="actionbar">
  <div class="left">
    Like (8)
  </div>
    <div class="right">
      <%= time_ago_in_words(cooperative.created_at)%>
    </div>

  </div>
  </div>
  
  <% end %>
  </div>
  